{
  "cli": {
    "version": ">= 3.1.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "android": {
        "env": {
          "GOOGLE_SERVICES_FILE": "./google-services.json"
        }
      },
      "ios": {
        "env": {
          "GOOGLE_SERVICES_FILE": "./GoogleService-Info.plist"
        },
        "resourceClass": "m1-medium"
      }
  }
  },
  "submit": {
    "production": {}
  }
}
