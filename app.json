{
  "expo": {
    "name": "Akhar: Punjabi Games",
    "slug": "game",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/akhar.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#274C7C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.khalisfoundation.gurmukhi-games",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "34"
    },
    "android": {
      "package": "com.khalisfoundation.gurmukhi_games",
      "googleServicesFile": "./google-services.json",
      "versionCode": 34,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-akhar.png",
        "backgroundColor": "#274C7C"
      }
    },
    "extra": {
      "eas": {
        "projectId": "d2512b2e-7df8-42a1-b0de-5e11fe888618"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ]
  }
}
